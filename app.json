{
  "expo": {
    "name": "TechChallenge Financeiro",
    "slug": "techchallenge-financeiro",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techchallenge.financeiro"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.techchallenge.financeiro"
    },
    "web": {},
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "A aplicação precisa acessar suas fotos para anexar recibos às transações."
        }
      ]
    ]
  }
}

